{
  "name": "chorestree",
  "version": "1.0.0",
  "description": "Discord bot for managing chores and tasks",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"platform/**/*.ts\"",
    "clean": "rimraf dist coverage",
    "analyze:perf": "ts-node -r tsconfig-paths/register src/tools/run-performance-analysis.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:v2:migration:generate": "ts-node -r tsconfig-paths/register src/v2/core/database/migrations/scripts/generate.ts",
    "db:v2:migration:run": "ts-node -r tsconfig-paths/register src/v2/core/database/migrations/scripts/run.ts",
    "db:v2:migration:revert": "ts-node -r tsconfig-paths/register src/v2/core/database/migrations/scripts/run.ts down",
    "db:v2:migration:validate": "ts-node -r tsconfig-paths/register src/v2/core/database/migrations/scripts/run.ts validate",
    "db:v2:migration:status": "ts-node -r tsconfig-paths/register src/v2/core/database/migrations/scripts/status.ts",
    "docker:db:up": "docker-compose up -d postgres redis",
    "docker:db:down": "docker-compose down -v",
    "prepare": "husky"
  },
  "keywords": [
    "discord",
    "bot",
    "chores",
    "tasks",
    "management"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/amqplib": "^0.10.7",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.12",
    "@nestjs/core": "^11.0.12",
    "@nestjs/testing": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "@types/cache-manager-redis-store": "^3.0.0",
    "@types/cron": "^2.4.3",
    "@types/datadog-metrics": "^0.11.0",
    "amqplib": "^0.10.5",
    "cache-manager": "^6.4.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-validator": "^0.14.1",
    "cron": "^4.1.0",
    "datadog-metrics": "^0.12.1",
    "discord.js": "^14.18.0",
    "ioredis": "^5.6.0",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21",
    "zod": "^3.24.2"
  }
}
